    "react-dom": "18.2.0",
    "react-dropzone": "^14.3.8",
    "react-hot-toast": "^2.4.1",
    "react-input-mask": "^2.0.4",
    "react-router-dom": "6.30.1",
    "react-toastify": "^11.0.5",
